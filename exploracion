        <script type="text/javascript">
            $(document).on("ready", function () {
                $('#menu-menu-noticias').slicknav({
                    prependTo: '#menu-mobile-noticias-box'
                });
                $('#menu-menu-principal').slicknav({
                    prependTo: '#menu-mobile-principal-box'
                });


                menu_noticias = false;
                $("#menu-mobile-noticias").on("click", function () {
                    menu_principal = false;
                    $("#menu-mobile-principal-box").css("display", "none");
                    if (menu_noticias) {
                        menu_noticias = false;
                        $("#menu-mobile-noticias-box").css("display", "none");
                    } else {
                        menu_noticias = true;
                        $("#menu-mobile-noticias-box").css("display", "block");
                    }
                });

                menu_principal = false;
                $("#menu-mobile-principal").on("click", function () {
                    menu_noticias = false;
                    $("#menu-mobile-noticias-box").css("display", "none");
                    if (menu_principal) {
                        menu_principal = false;
                        $("#menu-mobile-principal-box").css("display", "none");
                    } else {
                        menu_principal = true;
                        $("#menu-mobile-principal-box").css("display", "block");
                    }
                });

                var uri = window.location.href;
                /*  if (uri=='https://www.talca.cl/') {
                 $('.bs-example-modal-lg').modal('toggle')
                 }
                 */

            });

        </script>
